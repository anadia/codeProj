<!DOCTYPE html>
<html>
  <head>
    <title>Users</title>
    <link rel='stylesheet' href="/stylesheets/style.css" />
  </head>
  <body>

 
    <table id="table" >  
      <thead>  
          <tr>  
              <th>User ID</th>  
              <th>Name</th>
              <th>Lastname</th>
              <th>Email</th>
              <th>Phone</th>           
          </tr>  
      </thead>  
       <tbody>  
       <% users.forEach(function (user) { %>
          <tr>  
              <td><%= user.user_id %></td>  
              <td><%= user.name %></td>
              <td><%= user.lastname %></td>
              <td><%= user.email %></td>
              <td><%= user.phone %></td>
              <td> <a href="users/delete/<%= user.user_id %>">Eliminar</a></td>
              <td> <a href="users/edit/<%= user.user_id %>">Editar</a></td>
          </tr>                                   
       <% }) %>
       </tbody>
  </table>   
  <h3><a href="/articles"> Go to articles</a></h3>
  <h3><a href="/"> Go to Home</a></h3>
  <form action="/users" method="post">
    <input type="text" name="name">  
    <input type="text" name="lastname">  
    <input type="text" name="email">  
    <input type="text" name="phone">  
    <input type="submit" name="submit">  
  
  </form>
  </body>
</html>
