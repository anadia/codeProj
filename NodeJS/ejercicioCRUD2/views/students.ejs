  <h4>STUDENTS</h4>
  <div class="container">
    <div class="row">
      
  <div class="col-12">
    <form class="tal">
    <table class="table-dark pdlf">
      <thead>
        <tr>
          <th>Id</th>
          <th>Name</th>
          <th>Lastname</th>
          <th>Eyes Color</th>
          <th>Nationality</th>
          <th>Age</th>
          <th>Subject</th>
          <th class="wdt"></th>
          <th class="wdt"></th>
        </tr>
      </thead>
      <tbody>

        <tr>
          <% students.forEach(function (studata) { %>
          <td> <input type="text" value="<%= studata.id %>" disabled></td>
          <td> <input type="text" value="<%= studata.name %>" disabled></td>
          <td> <input type="text" value="<%= studata.lastname %>" disabled></td>
          <td> <input type="text" value="<%= studata.eyescolor %>" disabled></td>
          <td> <input type="text" value="<%= studata.nationality %>" disabled></td>
          <td> <input type="text" value="<%= studata.age %>" disabled></td>
          <!-- <% students.forEach(function (subjects) { %>  <td> <input type="text" value="<%= subjects.subjects_name %>" disabled></td> <% }) %> -->
          <td> <a href="students/delete/<%= studata.id %>"><button type="button" class="btn btn-dark">Delete</button></a></td>
          <td> <a href="students/edit/<%= studata.id %>"><button type="button" class="btn btn-dark">Edit</button></a></td>
        </tr>
        <% }) %>
      </tbody>

    </table>
    </form>
